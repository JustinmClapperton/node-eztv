  "main": "index.js",
  "keywords": [
    "eztv",
    "api",
    "torrents"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib",
    "clean": "rm -rf lib",
    "lint": "cross-env NODE_ENV=test eslint src test --fix",
    "preversion": "npm run build",
    "spec": "cross-env NODE_ENV=test mocha --retries 1 --timeout 10000 --bail --inline-diffs --async-only --growl --compilers js:babel-register --recursive test/*.spec.js",
    "test": "cross-env NODE_ENV=test npm run lint && npm run build && npm run spec"
  },
